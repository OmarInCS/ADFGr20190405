<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="DeptEmp-task-flow-definition">
    <default-activity>DeptEmpPage</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="DeptEmpPage">
      <page>/DeptEmpPage.jsff</page>
    </view>
    <view id="Query">
      <page>/Query.jsff</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>Query</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>back</from-outcome>
        <to-activity-id>DeptEmpPage</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>DeptEmpPage</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>goQuery</from-outcome>
        <to-activity-id>Query</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
